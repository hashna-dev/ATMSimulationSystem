spring.application.name=MySpringDataJPA
spring.datasource.url=jdbc://localhost:3306/userdb
spring.datasource.username=root
spring.datasource.password=root
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQLDialet
spring.jpa.hibernate.ddl-auto=update
